{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "blue",
          "style": "plain",
          "template": "@{{ .UserName }} ",
          "type": "session"
        },
        {
          "foreground": "white",
          "properties": {
            "style": "agnoster_short"
          },
          "style": "plain",
          "template": "{{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "white",
          "properties": {
            "style": "agnoster_short"
          },
          "style": "plain",
          "template": ": ",
          "type": "path"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#56B6C2",
          "properties": {
            "branch_icon": ""
          },
          "style": "plain",
          "template": "<#E8CC97>git(</>{{ .HEAD }}<#E8CC97>) </>",
          "type": "git"
        }
      ],
      "type": "rprompt"
    }
  ],
  "final_space": true,
  "version": 2
}
